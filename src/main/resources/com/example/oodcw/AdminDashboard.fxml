<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.TableCell?>
<?import javafx.scene.control.TableColumn.CellDataFeatures?>
<?import javafx.util.Callback?>

<AnchorPane prefHeight="370.0" prefWidth="442.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.oodcw.AdminDashboard">
    <TableView fx:id="userTable" layoutX="10.0" layoutY="10.0" prefHeight="300.0" prefWidth="424.0">
        <columns>
            <TableColumn fx:id="nameColumn" text="Name" prefWidth="200.0" />
            <TableColumn fx:id="usernameColumn" text="Username" minWidth="0.0" prefWidth="221.0" />
        </columns>
    </TableView>

    <Button fx:id="addUserButton" layoutX="10.0" layoutY="320.0" mnemonicParsing="false" onAction="#handleAddUser" text="Add User" />

    <Button fx:id="deleteUserButton" layoutX="100.0" layoutY="320.0" mnemonicParsing="false" onAction="#handleDeleteUser" text="Delete User" />
</AnchorPane>
